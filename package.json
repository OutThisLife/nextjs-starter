{
  "name": "nextjs-starter",
  "repository": "https://github.com/OutThisLife/nextjs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@mars/heroku-nextjs-build": "^1.1.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "https://registry.npmjs.org/babelify/-/babelify-7.3.0.tgz",
    "css-loader": "^0.26.1",
    "next": "^2.0.0-beta.26",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.1",
    "skeleton-loader": "0.0.7",
    "style-loader": "^0.13.1",
    "val-loader": "^0.5.0"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "live-reload": "^1.1.0",
    "parallelshell": "^2.0.0",
    "uglifyify": "^3.0.4",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parallelshell \"next\" \"npm run watch:js\" \"npm run watch:css\" \"npm run live-reload\"",
    "build": "next build",
    "start": "next start -p $PORT",
    "build:js": "browserify -t babelify -t uglifyify assets/js/main.js -o static/bundle.js",
    "watch:js": "watchify assets/js/main.js -t babelify -o static/bundle.js -dv",
    "css": "node-sass --include-path css --output-style compressed assets/css/main.sass static/main.css",
    "watch:css": "npm run css && npm run css -- --w",
    "live-reload": "live-reload --port 9091 static/",
    "heroku-postbuild": "heroku-nextjs-build"
  },
  "engines": {
    "node": "6.9.x"
  },
  "cacheDirectories": [
    "nextjs/node_modules"
  ],
  "author": "",
  "license": "ISC"
}
